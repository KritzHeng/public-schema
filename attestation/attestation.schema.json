{
  "$id": "EB4AsU1rKGOAf7m4MS324XhanXq8G01sR_bUdUV2TULm",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Data Attestation Credential",
  "description": "Data attestation credential enables verifiable attestation of the data(cryptographic digest) by the Issuer",
  "type": "object",
  "credentialType": "DataAttestationCredential",
  "version": "1.0.0",
  "properties": {
    "v": {
      "description": "Version",
      "type": "string"
    },
    "d": {
      "description": "Credential SAID",
      "type": "string"
    },
    "i": {
      "description": "Issuer AID",
      "type": "string"
    },
    "ri": {
      "description": "Credential status registry",
      "type": "string"
    },
    "s": {
      "description": "schema SAID",
      "type": "string"
    },
    "a": {
      "$id": "ENhgUPygyvCD3yllT62KHZofN0zcKUoMoi2IEbm6tdDz",
      "description": "Attributes block",
      "type": "object",
      "properties": {
        "d": {
          "description": "Attributes block SAID",
          "type": "string"
        },
        "dt": {
          "description": "Issuance date time",
          "format": "date-time",
          "type": "string"
        },
        "digest": {
          "type": "string",
          "description": "CESR-encoded digest of the data."
        }
      },
      "additionalProperties": false,
      "required": [
        "d",
        "dt",
        "digest"
      ]
    },
    "e": {
      "oneOf": [
        {
          "description": "Edges block SAID",
          "type": "string"
        },
        {
          "$id": "ELSumn6IoIYKIvHRwsV4kVjfpcTW0XhOU0Z7iQ_WIeZs",
          "description": "Edges block",
          "type": "object",
          "required": [
            "d",
            "u",
            "issuer"
          ],
          "properties": {
            "d": {
              "description": "Edges block SAID",
              "type": "string"
            },
            "u": {
              "description": "A salty nonce",
              "type": "string"
            },
            "issuer": {
              "description": "SAID of a credential that proves the identity of the issuer, if chaining is desired.",
              "type": "string"
            }
          },
          "additionalProperties": false
        }
      ]
    }
  },
  "additionalProperties": false,
  "required": [
    "i",
    "ri",
    "s",
    "d"
  ]
}